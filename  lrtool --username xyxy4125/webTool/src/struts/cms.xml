<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="cms" extends="struts-default">
        
       <action name="showNewsList" class="com.base.core.action.fo.showNewsAction" method="listNews">
			<result name="success">/fo/newsList.jsp</result>
			<result name="lr_space">/fo/spaceNewsList.jsp</result>
		</action>

		<action name="showDetail" class="com.base.core.action.fo.showNewsAction" method="showDetail">
			<result name="success">/fo/newsDetail.jsp</result>
			<result name="lr_space">/fo/spaceNewsDetail.jsp</result>
		</action>

	</package>

</struts>
