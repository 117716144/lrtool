<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.1//EN"
        "http://tuckey.org/res/dtds/urlrewrite3.1.dtd">


<urlrewrite>

    <outbound-rule>
        <from>^/(\w+)\.jsp\?planType=(\w+)($|\/$)</from>
        <to>/$1\/</to>
    </outbound-rule>

    <rule>
        <from>^/index($|\/$)</from>
        <to>/indexAction.action</to>
    </rule>
    
</urlrewrite>